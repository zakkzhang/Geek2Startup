
<view class="updateVideo video" >

	<view class="head {{uiClass.video}}">
		<video class="video-view"src="{{src}}"objectFit="cover"controls ></video>
	</view>

	<view class="empty {{uiClass.empty}}">
		<view class="p">{{uiText.tips}}</view>
		<view class="image2">
			<image src="data:image/png;base64," />
		</view>
	</view>

	<form bindsubmit="clickSave">

	<view class="form {{uiClass.video}}">
		<view class="title b">视频名称：</view>
		<input name="videoName" placeholder="视频名称" />
		<hr/>
		<view class="title b">时长：</view>
		<view class="p" >{{video.duration}}</view>
		<input class="hidden" name="duration" value="{{video.duration}}" />
	</view>

	<view class="control" >
		<view class="button {{uiClass.emptyControl}}" bindtap="clickGo" >{{uiText.mainButton}}</view>
		<button class="button {{uiClass.video}}" formType="submit">确认上传</button>
		<view class="button second {{uiClass.emptyControl}}" bindtap="needHelp" >需要帮助我制作一个</view>
		<view class="button second {{uiClass.video}}" bindtap="clickRedo" >重新选择</view>
		<view class="button {{uiClass.done}}"  bindtap="playNow" >查看视频页</view>
	</view>

	</form>

</view>
